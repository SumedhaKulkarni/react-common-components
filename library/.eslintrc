{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
      "browser": true,
      "jest": true
  },
  "rules": {
      "react/jsx-filename-extension": "off",
      "jsx-a11y/label-has-for": [ 2, {
        "required": {
            "every": [ "id" ]
        }
    }]
  }
}